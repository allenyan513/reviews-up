{
  "name": "@reviewsup/embed-react",
  "version": "0.0.2",
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "tsc -b -v --watch",
    "build": "tsc && npm run reviewsup-react:copy",
    "reviewsup-react:copy": "mkdir -p dist/styles && cp -r src/styles/* dist/styles",
    "clean": "rm -rf dist && rm -rf .turbo"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "*": []
      }
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@repo/ui": "*"
  },
  "devDependencies": {
    "@repo/api": "*",
    "@repo/database": "*"
  }
}
