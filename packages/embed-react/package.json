{
  "name": "@reviewsup/embed-react",
  "version": "0.0.3",
  "private": false,
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css --watch",
    "dev:components": "tsc --watch",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/index.css",
    "build:components": "tsc",
    "publish": "pnpm build:components && pnpm build:styles && npm publish --access public"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.{js,d.ts,css}"
  ],
  "exports": {
    "./styles.css": "./dist/index.css",
    ".": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "typesVersions": {
      "*": {
        "index": [
          "dist/index.d.ts"
        ],
        "*": []
      }
    }
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0"
  },
  "dependencies": {
    "@reviewsup/api": "workspace:*"
  }
}
